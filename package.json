{
  "name": "nearblocks",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:check": "turbo run lint:check",
    "format": "prettier --config .prettierrc --write --cache \"**/*.{js,cjs,jsx,ts,tsx,md,json}\"",
    "format:check": "prettier --config .prettierrc --check --cache \"**/*.{js,cjs,jsx,ts,tsx,md,json}\"",
    "prepare": "husky install"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint-config-custom-nextjs": "*",
    "eslint-config-custom-node": "*",
    "prettier": "3.0.3",
    "turbo": "1.10.16",
    "husky": "8.0.0"
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
