    "lint": "tsc --noEmit && eslint ./ --fix"
  },
  "dependencies": {
    "@scure/base": "1.1.3",
    "@sentry/node": "7.74.1",
    "axios": "1.5.1",
    "envalid": "8.0.0",
    "lodash-es": "4.17.21",
    "near-lake-framework": "2.0.0",
    "stream-json": "1.8.0"
  },
  "devDependencies": {
    "@types/lodash-es": "~4.17",
    "@types/node": "~20.8",
    "@types/stream-json": "~1.7",
    "cpy-cli": "~5.0",
    "eslint-config-custom-node": "*",
    "nodemon": "~3.0",
    "rimraf": "~5.0",
    "nb-knex": "*",
    "nb-logger": "*",
    "nb-redis": "*",
    "nb-tsconfig": "*",
    "nb-types": "*",
    "nb-utils": "*",
    "typescript": "~5.2"
  }
}
