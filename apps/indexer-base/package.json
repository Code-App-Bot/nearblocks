{
  "name": "indexer-base",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "imports": {
    "#*": [
      "./dist/*.js"
    ]
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && tsc",
    "start": "node dist/index.js",
